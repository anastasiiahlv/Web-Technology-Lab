@model List<ProjectManagementSystem.Models.Task>

<div class="row" id="taskList">
    @if (Model.Any())
    {
        @foreach (var task in Model)
        {
            <div class="col-md-4 mb-4">
                @Html.Partial("_TaskCard", task)
            </div>
        }
    }
    else
    {
        <p class="text-muted">Завдань немає.</p>
    }
</div>



